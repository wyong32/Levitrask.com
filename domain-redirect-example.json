{
  "version": 2,
  "redirects": [
    {
      "source": "/author-(.+)\\.html",
      "destination": "/en/404",
      "permanent": true
    },
    {
      "source": "/date-(.+)\\.html",
      "destination": "/en/404",
      "permanent": true
    },
    {
      "source": "/post/(.+)\\.html",
      "destination": "/en/404",
      "permanent": true
    },
    {
      "source": "/tags-(.+)\\.html",
      "destination": "/en/404",
      "permanent": true
    },
    {
      "source": "/category-(.+)\\.html",
      "destination": "/en/404",
      "permanent": true
    },
    
    {
      "source": "/(.*)",
      "destination": "/en/404",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "your-unused-domain1.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/en/404", 
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "your-unused-domain2.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/en/404",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "your-unused-domain3.com"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/|_next/|static/|public/|assets/|favicon.ico|robots.txt|sitemap.xml|[\\w-]+\\.\\w+).*)",
      "destination": "/index.html"
    }
  ]
}
