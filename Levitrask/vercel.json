{
  "version": 2,
  "rewrites": [
    {
      "source": "/author-(.+)\\.html",
      "destination": "/index.html",
      "status": 404
    },
    {
      "source": "/date-(.+)\\.html",
      "destination": "/index.html",
      "status": 404
    },
    {
      "source": "/post/(.+)\\.html",
      "destination": "/index.html",
      "status": 404
    },
    {
      "source": "/tags-(.+)\\.html",
      "destination": "/index.html",
      "status": 404
    },
    {
      "source": "/category-(.+)\\.html",
      "destination": "/index.html",
      "status": 404
    },
    {
      "source": "/((?!api/|_next/|static/|public/|assets/|favicon.ico|robots.txt|sitemap.xml|[\\w-]+\\.\\w+).*)",
      "destination": "/index.html"
    }
  ]
}
